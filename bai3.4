using System;

namespace ConsoleApp1
{
    internal class Program
    {
        private static void Main(string[] args)
        {
            int a = int.Parse(Console.ReadLine());
            int[] mang = new int[a];
            for (int i = 0; i < a; i++)
            {
                Console.WriteLine($"Nhap gia tri cho phan tu thu {i + 1}");
                
                string input = Console.ReadLine();
                if (int.TryParse(input, out int value))
                {
                    mang[i] = value;
                }
                else
                {
                    Console.WriteLine("Khong hop le, nhap lai di bro");
                    
                    i--;

                }

            }
            Console.WriteLine("danh sach trong mang");
            for (int j = 0; j < a; j++)
            {
                Console.WriteLine($"{mang[j]}");
            
            }
            Console.WriteLine("Phan tu can kiem tra: ");
            int l = int.Parse(Console.ReadLine());
            int count = 0;
            for (int i = 0; i < a; i++)
            {
                if (mang[i] == l)
                {
                    count++;
                    Console.WriteLine($"{i}");
                }
            }
            if (count == 0)
            {
                Console.WriteLine("Khong co phan tu trong mang");
                Console.ReadKey();
            }
            if (count > 0)
            {
                Console.WriteLine($"So lan phan tu xuat hien: {count}");
                Console.ReadKey();
            }
        }
    }
}
